<div class="modal fade" id="modallojaalterar" tabindex="-1" role="dialog" aria-labelledby="ModalLojaAlterar" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Fechar</span></button>
                <h4 class="modal-title" id="myModalLabel">Alterar Loja <span id="lblCnpj"></span></h4>
            </div>
            <div class="modal-body">
                <input type="hidden" id="LojaID" />
                <div class="form-group">
                    <div class="row">
                        <div class="col-lg-10">
                            <input id="Cnpj" name="Cnpj" type="text" class="form-control cnpj" placeholder="Cnpj" maxlength="18" />
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-lg-10">
                            <input id="RazaoSocial" name="RazaoSocial" type="text" class="form-control" placeholder="Razão Social" maxlength="150" />
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-lg-10">
                            <input id="NomeFantasia" name="NomeFantasia" type="text" class="form-control" placeholder="Nome Fantasia" maxlength="150" />
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-lg-10">
                            <input id="Telefone" name="Telefone" type="text" class="form-control fixo" placeholder="Telefone" maxlength="10" />
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-lg-10">
                            <input id="Bairro" name="Bairro" type="text" class="form-control" placeholder="Bairro" maxlength="75" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="btnAlterarLoja" type="button" class="btn btn-warning">Alterar</button>
            </div>
        </div>
    </div>
</div>